    "test": "tsc && node dist/test/test-e2e.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "lightning",
    "bitcoin",
    "l402",
    "lsat",
    "macaroon",
    "ai-agent",
    "payments",
    "lnd",
    "micropayments"
  ],
  "author": "satpath",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/satpathdev/gateless"
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@types/node": "^25.3.0",
    "typescript": "^5.9.3"
  }
}
